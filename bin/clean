#!/bin/bash
#
# $Id: import_file 655 2008-04-09 07:50:42Z haftmann $
# Author: Florian Haftmann, TU Muenchen
#
# Call to importer from the shell.

function fail()
{
  echo "$1" >&2
  exit 2
}

EXECPATH="$(dirname "$0")/.."

[ -z "$EXECPATH" ] && fail "$0 must be called with explicit path"

cd "$EXECPATH"
rm -v ex/dst_thy/* ex/dst_hs/*
